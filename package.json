{
  "name": "element-components",
  "version": "1.0.19",
  "main": "./src/components/index.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-components": "vue-cli-service build --target lib --name elCompontents ./src/components/index.js --dest dist-compoents",
    "build-table-template": "vue-cli-service build --target lib --name tableTemplate ./src/components/tableTemplate/export.js --dest dist-table",
    "build-tinymce": "vue-cli-service build --target lib --name tinymce ./src/components/Tinymce/export.js --dest dist-tinymce",
    "build-upload-box": "vue-cli-service build --target lib --name uploadBox ./src/components/uploadBox/export.js --dest dist-uploadBox"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "element-ui": "^2.12.0",
    "js-utils": "git+http://git.tigoyun.com/zzhijie/js-utils.git",
    "vue": "^2.6.10",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "vue-template-compiler": "^2.6.10"
  }
}
